{
  "name": "Shadowsocks NaCl Test flight",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Shadowsocks Nacl Test flight",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": [ "src/shadowsocks.js", "tests/test_flight.js" ]
    }
  },
  "permissions": [
    {
        "socket": [
            "tcp-listen:*:*",
            "tcp-connect",
            "resolve-host",
            "udp-bind:*:*",
            "udp-send-to:*:*"
        ]
    }
  ]
}
